//33) Ler dois valores e imprimir uma das três mensagens a seguir:
//‘Números iguais’, caso os números sejam iguais
//‘Primeiro é maior’, caso o primeiro seja maior que o segundo; ‘Segundo maior’, caso o segundo seja maior que o primeiro.



package ExercicioFacct;

import java.util.Scanner;

public class Exercicio33 {

	public static void main(String[] args) {
		Scanner sc = new Scanner (System.in);
				
				System.out.println("Digite o primeiro valor: ");
				int valor1=sc.nextInt();
				
				System.out.println("\nDigite o primeiro valor: ");
				int valor2=sc.nextInt();
				
				if (valor1==valor2) {
					System.out.println("\nNumeros iguais\n");
						
					}else if(valor1>valor2) {
						System.out.println("\nO Primeiro valor é maior");
					}else {
						System.out.println("\nO Segundo valor é maior");
					}
				sc.close();
					
				}
					
			}
			
		 
		 
		 
